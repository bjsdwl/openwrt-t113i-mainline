CONFIG_TARGET_sunxi=y
CONFIG_TARGET_sunxi_cortexa7=y
CONFIG_TARGET_sunxi_cortexa7_DEVICE_allwinner_t113-s3=y

# 关键：必须显式取消默认的 BPI-M3，并指定 T113-S3 变体
# 注意：OpenWrt 的 uboot 包名通常遵循 uboot-sunxi-<Variant> 格式
CONFIG_PACKAGE_uboot-sunxi-allwinner_t113_s3=y
# 取消通配符包，防止多余编译
# CONFIG_PACKAGE_uboot-sunxi is not set
