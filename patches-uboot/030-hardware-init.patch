+		*(volatile unsigned int *)0x02000128 = 0x77777777; /* PG_CFG2 UART0 */
+		*(volatile unsigned int *)0x02000124 = (*(volatile unsigned int *)0x02000124 & ~(0xF << 12)) | (0x1 << 12);
