Index: u-boot-2025.01/arch/arm/dts/sun8i-t113-tronlong.dts
===================================================================
--- /dev/null
+++ u-boot-2025.01/arch/arm/dts/sun8i-t113-tronlong.dts
@@ -0,0 +1,40 @@
+/dts-v1/;
+#include "sun8i-t113s.dtsi"
+#include <dt-bindings/gpio/gpio.h>
+#include <dt-bindings/leds/common.h>
+
+/ {
+    model = "Tronlong TLT113-MiniEVM";
+    compatible = "tronlong,tlt113-minievm", "allwinner,sun8i-t113i", "allwinner,sun8i-t113s";
+    aliases { };
+
+    leds {
+        compatible = "gpio-leds";
+        bootph-all;
+        
+        led-0 {
+            label = "heartbeat";
+            gpios = <&pio 2 0 GPIO_ACTIVE_HIGH>; /* PC0 */
+            default-state = "on";
+            bootph-all;
+        };
+    };
+};
+
+/* 引用硬件 */
+&ccu { bootph-all; };
+&pio { bootph-all; };
+
+/* 修复：通过路径覆盖 clocks 容器，而不是引用 &osc24M */
+/* 在 sun8i-t113s.dtsi 中，osc24M 定义在 /clocks/osc24M 还是根目录下？ */
+/* 通常全志 DTS 把 osc24M 放在根目录下。我们尝试给根目录下的 osc24M 节点加标签 */
+
+/* 尝试 1: 假设它叫 osc24M */
+/* 如果报错，说明 dtsi 里没定义这个节点，或者名字不一样 */
+/* 我们直接在根节点里追加 bootph-all */
+
+/ {
+    osc24M {
+        bootph-all;
+    };
+};
